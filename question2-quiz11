import string
count = 0
x = 'BANANAS'


archivo = open('bananas.txt','r')
line = archivo.readline()
str.upper(line)
lines = archivo.readlines()
" ".join(line)


line = line.split(' ')
for c in line:
	if c == x:
		count += 1
	
print (count)

archivo.close
